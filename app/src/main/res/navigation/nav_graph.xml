<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/loginFragment">

    <fragment
        android:id="@+id/loginFragment"
        android:name="com.me.quiz.telas.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_loginFragment_to_cadastroFragment5"
            app:destination="@id/cadastroFragment" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.me.quiz.telas.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_quizFragment"
            app:destination="@id/quizFragment" />
        <action
            android:id="@+id/action_homeFragment_to_perfilFragment"
            app:destination="@id/perfilFragment" />
        <action
            android:id="@+id/action_homeFragment_to_painelFragment"
            app:destination="@id/painelFragment" />
    </fragment>
    <fragment
        android:id="@+id/quizFragment"
        android:name="com.me.quiz.telas.QuizFragment"
        android:label="fragment_quiz"
        tools:layout="@layout/fragment_quiz" />
    <fragment
        android:id="@+id/perfilFragment"
        android:name="com.me.quiz.telas.PerfilFragment"
        android:label="fragment_perfil"
        tools:layout="@layout/fragment_perfil" />
    <fragment
        android:id="@+id/cadastroFragment"
        android:name="com.me.quiz.telas.CadastroFragment"
        android:label="fragment_cadastro"
        tools:layout="@layout/fragment_cadastro">
        <action
            android:id="@+id/action_cadastroFragment5_to_loginFragment"
            app:destination="@id/loginFragment"/>
    </fragment>
    <fragment
        android:id="@+id/painelFragment"
        android:name="com.me.quiz.telas.PainelFragment"
        android:label="fragment_painel"
        tools:layout="@layout/fragment_painel" />
</navigation>